total = 0

print("Nhập giao dịch (D n hoặc W n). Nhập dòng trống để kết thúc:")

while True:
    line = input()
    if line == "":
        break

    action, amount = line.split()
    amount = int(amount)

    if action == "D":
        total += amount
    elif action == "W":
        total -= amount

print("Số dư cuối cùng:", total)
